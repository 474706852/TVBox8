    "cateVodImgR": "\\S+(http\\S+)",
    "cateVodMark": "//span[contains(@class,'pic-text')]/b/text()",
    
// Category Details ================================================
    "dtUrl": "https://www.tangrenjie.tv/vod/detail/id/{vid}.html",
    "dtNode":  "//body",
    "dtName":  "//div[contains(@class,'content_thumb')]/a/@title",
    "dtNameR": "",
    "dtImg":   "//div[contains(@class,'content_thumb')]/a/@data-original",
    "dtImgR":  "\\S+(http\\S+)",
    "dtCate":     "//li[@class='data']/span[contains(text(),'类型')]",
    "dtYear":     "//li[@class='data']/span[contains(text(),'年份')]/following-sibling::*/text()",
    "dtArea":     "//li[@class='data']/span[contains(text(),'地区')]/following-sibling::*/text()",
    "dtMark":     "//li[@class='data']/span[contains(text(),'更新')]/following-sibling::*/text()",
    "dtDirector": "concat(//li[@class='data']/span[contains(text(),'导演')]/following-sibling::a[position()<6]/text())",
    "dtActor":    "concat(//li[@class='data']/span[contains(text(),'主演')]/following-sibling::a[position()<6]/text())",
    "dtDesc":     "//div[@class='content']/section", #/section/font/p中可能没有/p节点，用/section下所有文本作为剧情介绍
    
// Playlist =====================================================
    "dtFromNode":   "//div[contains(@class,'play_source_tab')]/a/b",
    "dtFromName":   "/text()",
    "dtFromNameR":  "",
    "dtUrlNode":    "//div[@class='playlist_full']",
    "dtUrlSubNode": "/ul/li/a",
    "dtUrlId":      "/@href",
    "dtUrlIdR":     "/vod/play/id/(\\S+).html",
    "dtUrlName":    "/text()",
    "dtUrlNameR":   "",
    "playUrl": "https://www.tangrenjie.tv/vod/play/id/{playUrl}.html",
    "playUa": "",
    
// Search Results ================================================
